/* 首页样式 */
@font-face{
font-family:'COLLECTION3';
src: url('DESIGN.COLLECTION3.KLONDIKE.TTF');
}
.box-body{
	width:100%;
	height:100%;
	background:#030d26;
}
.box-body .content{
	width:100%;
	height:calc(100% - 1.66rem);	
	margin-top:0.5rem;
}
.left-content{
	width:8.66rem;
	height:calc(100% - 0.4rem + 0.3rem);
    background-image: url(../../images/bg_left.png);
    background-size: 100% 100%;
}

.right-content{
	width:9.22rem;
	height:calc(100% - 0.4rem);
	background-image: url(../../images/bg_right.png);
    background-size: 100% 100%;
    margin-left:0.42rem;
}
.right-content .right-box{
	width:100%;
	height:90%;
	margin-top:4.63%;
}
.left-content .left-box{
    margin-top:4.63%;
    margin-left: 0.55rem;
    width: 90%;
    height: 87%;
}
.right-top, .right-bottom{width:100%;height:50%;}


/*节目内容分布小标题*/
.title-h{height:0.3rem; width:100%; color:#c5d8f5; font-size:0.26rem; line-height:0.3rem;margin-left:0.3rem;}
.title-h i{height:100%; float:left;}
.title-h i:nth-child(1){width:0.68rem; background-image:url(../../images/content_bgTitle.png); background-size:100% 100%; }

.program-box{height:calc(100% - 0.3rem); width:100%;}

.right-bottom .left-bottom{width:47%; height:100%;}
.right-bottom .right-bottom{width:52%; height:100%;}

.space-box{height:calc(100% - 0.3rem); width:100%; float:left;}
.bg-box{ position:relative; left:50%; top:50%; margin-left:-1.45rem; margin-top:-1.5rem; width:2.9rem; height:3.0rem; background-image:url('../../images/bg_kongjian1.png');  background-size: 100% 100%;}
.circular-box , .circular_right-box{
	font-family: 'COLLECTION3';
	width:1.4rem;
	height:1.4rem;
	position:absolute;
	left:50%;
	top:50%;
	margin-left:-0.7rem;
	margin-top:-0.7rem;
	border-radius:50%;
	background-image:url('../../images/bg_kongjian2.png');
	background-size:100% 100%;
	font-size:0.48rem;
	line-height:1.4rem;
	text-align:center;
	color:#ff8d8e;
}

/*空间占比块*/
.block-box{height:2.2rem; width:2.2rem; border-radius:50%; position:absolute; top:50%; left:50%; margin-left:-1.1rem; margin-top:-1.1rem;}
.block-box span{width:0.26rem; height:0.38rem; position:absolute;background-image:url('../../images/bg_kongjian4.png'); background-size:100% 100%; left:50%; margin-left:-0.13rem;top:0; -webkit-transform-origin:center 1.1rem; -moz-transform-origin:center 1.1rem; -o-transform-origin:center 1.1rem; -ms-transform-origin:center 1.1rem;}
.block-box span.red{background-image:url('../../images/bg_kongjian3.png'); background-size:100% 100%; }


/*服务状态*/
.right-bottom  .space-left{width:3rem; height:3rem; background:url('../../images/bg_fuwu1.png'); background-size:100% 100%; position:relative; top:50%;  margin-top:-1.5rem;margin-left:0.3rem;opacity:0;}

.circular_right-box{
	background-image:url('../../images/bg_fuwu2.png');
	background-size:100% 100%;
	color:#6ea7fe;
}


.space-right{width:1.4rem; height:1.3rem; position:relative; top:50%; margin-top:-0.65rem; margin-left:0.09rem;font-size: 0.18rem; }
.space-right li{height:0.3rem; width:100%; margin-bottom:0.2rem;color:#6ea7fe;}
.space-right li:nth-child(2){color:#ff8d8e;}
.space-right li:nth-child(3){color:#fdfc99;}
.space-right li span{width:0.5rem; height:0.28rem; border:0.01rem solid #6ea7fe;display:inline-block;  box-shadow:0 0 0.1rem 0.01rem #6ea7fe inset;  text-align:center; line-height:0.3rem;  font-size:0.18rem; margin-right:0.1rem;}
.space-right li span.bg-span2{border:0.01rem solid #ff8d8e; box-shadow:0 0 0.1rem 0.01rem #ff8d8e inset;}
.space-right li span.bg-span3{border:0.01rem solid #fdfc99; box-shadow:0 0 0.1rem 0.01rem #fdfc99 inset;}
.space-right li span.bg-span4{border:0.01rem solid #5bed92; box-shadow:0 0 0.1rem 0.01rem #5bed92 inset;}

/*业务统计*/
.select-box{width:100%; height:0.56rem; }
.select-box .select-ul{width:3.2rem; height:100%;}
.select-box .select-ul li{height:100%; width:0.48rem; text-align:center; line-height:0.56rem; margin-right:0.2rem; font-size:0.2rem; color:#6ea7fe;cursor: pointer;}
.select-box .select-ul li.active{height:calc(100% - 0.02rem); border-bottom:0.02rem solid #cee2ff; color:#cee2ff;}
.month-box{
	position: relative;
    height: 6.5rem;
    display:none;
}

.bottom-ul{width:6.5rem; height:0.3rem;  position:absolute; bottom:12%; left:50%; margin-left:-3.25rem;}
.bottom-ul li{width:2.76rem; height:100%; margin-left:0.4rem; line-height:0.3rem; color:#6ea7fe; font-size:0.24rem;}
.bottom-ul li span{width:0.6rem; height:0.3rem; background-image:url('../../images/content_bgBofang.png'); display:inline-block;}
.bottom-ul li span.two{background-image:url('../../images/content_bgluzhi.png'); }
.bottom-ul li:last-child {color:#f47576;}

.bar-box{width:100%; height:5rem; }


.zhijin-box{width:100%; height:5.60rem;  display:none;}
.zhijin-ul{width:95%; height:4.60rem;  margin:0 auto; position:relative; background:url('../../images/line.png') center bottom no-repeat; background-size:100%;}
.zhijin-ul li{width:2.02rem; height:3.06rem; border:0.02rem solid #475c7b; margin-left:0.98rem; position:absolute; bottom:1.2%; }
.zhijin-ul li.zhijin-record{left:45%;border:0.02rem solid #674f4f;}
.zhijin-ul li span{width:2.02rem; color:#9bc3ff; font-size:0.52rem; height:0.6rem; line-height:0.6rem; text-align:center; position:absolute; top:-26%; left:50%; margin-left:-1.01rem;}
#recordCount-spans{width:2.02rem; color:#ff7a7a; font-size:0.52rem; height:0.6rem; line-height:0.6rem; text-align:center; position:absolute; top:-26%; left:50%; margin-left:-1.01rem;}
.txt-ul{width:95%; height:0.4rem; margin:0 auto; margin-top:0.2rem; font-size:0.22rem; position:relative; line-height:0.4rem; text-align:center;}
.txt-ul li{width:2.2rem; height:100%; margin-left:0.9rem; position:absolute; color:#9bc3ff;}
.txt-ul li.txt-record{left:45.5%; color:#f47576;}