    color: #cdd8f8;
    background-color: #18264f;
    border: 0.01rem solid #4f608d;
}

.add-delete-op{
	padding: 0.25rem 0;
    text-align: center;
}

.add-delete-op button{
	width:1.28rem;
}

.add-delete-op .sham-btn:first-child {
    margin-right: 1.7rem;
 }
 
 .add-delete-txt{
 	font-size:0.2rem;
 	color:#fff;
 	text-align:center;
 	line-height:0.6rem;
 }