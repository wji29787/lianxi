/* 云空间 */
i {
  font-style: normal;
}
.z-flag dl {
  padding-left: 0 !important;
}
.header-box {
  background-image: url(../../images/bg_nav_2.png?1) !important;
}
.layer-box {
  background-image: url(../../images/cloud_push.png?1) !important;
}
.content-list-thead-tab {
  height: 0.6rem;
  line-height: 0.6rem;
  background-color: #172449;
}
.content-list-thead-tab .content-list-thead-all {
  float: left;
}
.content-list-thead-tab .content-list-thead-all {
  float: left;
  font-size: .21rem;
  color: #e4ebf5;
}
.z-icon-box {
  float: left;
  width: 0.6rem;
  text-align: center;
  cursor: pointer;
}
.z-icon-box.z-h140 {
  line-height: 1.39rem;
}
.z-icon-box.z-h80 {
  line-height: 0.79rem;
}
.z-icon-box .icon-weixuanzhong {
  font-size: 0.32rem;
  color: #717fa5;
}
.z-icon-box .icon-xuanzhong {
  font-size: 0.32rem;
  color: #717fa5;
}
.t-add {
  display: none;
  padding-bottom: 30px;
  width: 500px;
  height: 150px;
  overflow: hidden;
}
.t-add .t-add-title {
  margin-top: 0;
  height: 36px;
  line-height: 36px;
  font-size: 20px;
  text-align: center;
  background-color: #717fa5;
}
.t-add .t-add-title i {
  float: right;
  width: 30px;
  height: 36px;
  color: #fff;
  font-size: 20px;
  cursor: pointer;
}
.t-add > div {
  margin-top: 25px;
  text-align: center;
}
.t-add .t-add-input {
  width: 80%;
  height: 34px;
  line-height: 34px;
  border: 1px solid #ccc;
  border-radius: 4px;
  font-size: 20px;
  color: #444;
  text-indent: 14px;
}
.t-add button {
  width: 150px;
  margin-right: 20px;
  height: 34px;
  line-height: 34px;
  border-radius: 4px;
  border: 1px solid #ccc;
  font-size: 20px;
  color: #fff;
  cursor: pointer;
}
.t-add button:nth-of-type(1) {
  background-color: #2d78f4;
}
.t-add button:nth-of-type(2) {
  background-color: #f54545;
}
