/******推送弹窗 start******/
.layer-box {
  width: 13rem;
  height: 7.14rem;
  background-image: url(../../images/bg_push.png?1);
  background-color: #0d1931;
  background-size: 100% 100%;
  /*推送头部*/
}
.layer-box .layer-top {
  width: 100%;
  height: 0.6rem;
  color: #ffffff;
  /*视频名字*/
  /*右上角 关不和刷新*/
}
.layer-box .layer-top .layer-title {
  height: 100%;
  width: 1.18rem;
  font-size: 0.22rem;
  line-height: 0.6rem;
  padding-left: 0.2rem;
}
.layer-box .layer-top .vod-name {
  height: 100%;
  width: calc(100% - 2 * 1.38rem);
  font-size: 0.22rem;
  line-height: 0.6rem;
  text-align: center;
}
.layer-box .layer-top .layer-topright {
  height: 100%;
  width: 1.2rem;
  font-size: 0.22rem;
  line-height: 0.6rem;
  padding-left: 0.18rem;
}
.layer-box .layer-top .layer-topright i {
  width: 0.6rem;
  height: 100%;
  text-align: center;
  line-height: 0.6rem;
}
.layer-box .layer-center {
  box-sizing: border-box;
  padding: 0.08rem 0.15rem 0;
  width: 100%;
  height: 5.24rem;
}
.layer-box .layer-center > img {
  width: 100%;
  height: 100%;
}
.layer-box .layer-center > div {
  width: 100%;
  height: 100%;
  background: url(http://192.168.10.49:8080/cms1/frame/2017-7-26/QP3020170726113822-924cb874-76b3-4258-be09-1d359d5392c5-frame.jpg) no-repeat center;
  background-size: 100% 100%;
}
.layer-box .layer-bottom {
  margin: 0 auto;
  width: 12.68rem;
  height: 1.3rem;
  overflow: hidden;
}
.layer-box .layer-bottom .input-box {
  width: 100%;
  margin-left: 0.62rem;
  height: 0.38rem;
  position: relative;
  margin-top: 0.18rem;
  margin-bottom: 0.11rem;
}
.layer-box .layer-bottom .input-box label {
  font-size: 0.18rem;
  color: #ffffff;
  line-height: 0.38rem;
  margin-right: 0.29rem;
  color: #b5b5b5;
}
.layer-box .layer-bottom .input-box input {
  height: calc(100% - 0.02rem);
  width: 3.1rem;
  border-radius: 0.04rem;
  border: 0.01rem solid #e8e8e8;
  position: absolute;
  top: 0;
  color: #e8e8e8;
  background: #050505;
}
.layer-box .layer-bottom .control-box {
  width: 100%;
  height: 0.46rem;
  margin-left: 0.57rem;
  position: relative;
}
.layer-box .layer-bottom .control-box .ifont {
  width: 0.4rem;
  height: 100%;
  font-size: 0.36rem;
  text-align: center;
  line-height: 0.46rem;
  color: #e8e8e8;
}
.layer-box .layer-bottom .control-box .zanting-play {
  margin-right: 0.23rem;
}
.layer-box .layer-bottom .control-box .when-long {
  height: 100%;
  text-align: center;
  line-height: 0.46rem;
  font-size: 0.16rem;
  color: #ddd;
  margin-left: 0.19rem;
  margin-right: 0.2rem;
}
.layer-box .layer-bottom .control-box .pro-bar-box {
  width: 7.12rem;
  height: 0.04rem;
  background: #e8e8e8;
  position: relative;
  top: 50%;
  margin-top: -0.02rem;
}
.layer-box .layer-bottom .control-box .pro-bar-box .pro-bar {
  width: 0%;
  height: 100%;
  background: #6780c2;
  position: absolute;
  top: 0;
  left: 0;
}
.layer-box .layer-bottom .control-box .pro-bar-box .pro-btn {
  width: 0.1rem;
  height: 0.1rem;
  border: 0.04rem solid #e8e8e8;
  border-radius: 50%;
  position: absolute;
  top: 50%;
  margin-top: -0.09rem;
}
.layer-box .layer-bottom .control-box .pro-bar-box .pro-btn .pro-minbtn {
  position: absolute;
  width: 0.1rem;
  height: 0.1rem;
  background: #6780c2;
  border-radius: 50%;
  top: 50%;
  margin-top: -0.05rem;
}
