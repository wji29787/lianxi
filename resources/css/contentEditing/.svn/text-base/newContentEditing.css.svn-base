} **/

/**内容区域**/
.resource-layer-bottom{
	padding:0 0.24rem;
}
.comm-layer-bottom{
	padding:0 0.25rem;
	overflow:hidden;
}

.layer-bottom-top{
	display:flex;
	justify-content:space-between;
	margin-top:0.07rem;
	height:0.6rem;
	overflow:hidden;
}

.layer-bottom-tab{
    display:flex;
    height:0.59rem;
    line-height:0.59rem;
    border-bottom:1px solid #4f608d;
	oveflow:hidden;
}

.layer-bottom-tab li{
    position:relative;
	width:1.94rem;
	text-align:center;
	color:#fff;
	font-size:0.22rem;
	cursor:pointer;
}

.layer-bottom-tab li.active,.layer-bottom-tab li:hover{
	color:#7fdbfd;
}

.layer-bottom-tab li.active:after,.layer-bottom-tab li:hover:after{
	content:'';
	position:absolute;
	left:0;
	bottom:-0.01rem;
	width:100%;
	height:0.02rem;
	background-color:#7fdbfd;
}

.layer-bottom-search{
	overflow:hidden;
	margin-top:0.22rem;
	margin-right:0.2rem;
}

.layer-bottom-search-input{
	float:left;
	box-sizing:border-box;
	padding:0 0.12rem;
	width:2.69rem;
	height:0.38rem;
	line-height:0.38rem;
	font-size:0.18rem;
	border:0.01rem solid #4a69a4;
	border-right:none;
	border-radius:0.04rem 0 0 0.04rem;
	background:none;
	color:#fff;
}

.layer-bottom-search-btn{
	float:left;
	width:0.41rem;
	height:0.38rem;
	line-height:0.38rem;
	font-size:0.20rem;
	text-align:center;
	border-radius:0 0.04rem 0.04rem 0;
	background-color:#4a69a4;
	color:#fff;
	cursor:pointer;
}

.layer-bottom-search-btn:hover{
	background-color:#4e5a7a;
}

.layer-bottom-content{
	padding-top:0.15rem;
}

.layer-bottom-full{
	overflow:hidden;
	height:0.52rem;
}

.layer-bottom-full i{
	float:left;
	width:0.6rem;
	height:0.52rem;
	line-height:0.52rem;
	text-align:center;
	font-size:0.3rem;
	color:#4867a1;
	cursor:pointer;
}

.layer-bottom-full span{
	float:left;
	line-height:0.52rem;
	font-size:0.22rem;
	color:#fff;
}

.layer-bottom-head{
    display:flex;
    padding-left:0.6rem;
	height:0.46rem;
	line-height:0.46rem;
	border-bottom:1px solid #374569;
	color:#fff;
}

.layer-bottom-head span{
	display:block;
	box-sizing:border-box;
	padding-right:0.12rem;
	font-size:0.22rem;
}

.layer-bottom-cell-name{
	width:3.56rem;
}

.layer-bottom-cell-stime{
	width:3.1rem;
}

.layer-bottom-cell-suser{
	width:1.97rem;
}

.layer-bottom-cell-time{
	width:1.36rem;
}

.layer-bottom-body{
	height:4.5rem;
	overflow-y:auto;
}

.layer-bottom-body .body-list{
	display:flex;
	padding:0.2rem 0;
	height:0.7rem;
	line-height:0.7rem;
	font-size:0.22rem;
	border-bottom:0.01rem solid #374569;
	color:#fff;
}

.layer-bottom-body .body-list:hover{
	box-shadow: 0 0 0.2rem #7fdbfd inset;
}

.layer-bottom-cell-radio{
	width:0.6rem;
	text-align:center;
}

.layer-bottom-cell-radio i{
	font-size:0.3rem;
	color:#4a69a4;
	cursor:pointer;
}

.body-list-img{
	display:flex;
}

.body-list-img img{
	display:block;
	margin-right:0.2rem;
	width:1.24rem;
	height:0.7rem;
    vertical-align: middle;
}

.body-list-img span{
	display:block;
	padding:0 0.12rem;
}

.layer-bottom-add{
	padding:0.25rem 0;
	text-align:center;
}

.layer-bottom-add button{
	width:1.88rem;
	height:0.38rem;
	line-height:0.38rem;
	font-size:0.2rem;
	color:#fff;
	text-align:center;
	border:none;
	background-color:#4a69a4;
	border-radius:0.04rem;
	cursor:pointer;
}

.layer-bottom-add button:hover{
	background-color:#4e5a7a;
}


/**皮肤选择弹层**/
.theme-box{
	width:12rem;
}

.theme-ul{
	padding:0.4rem 0 0 0.3rem;
	overflow:hidden;
}

.theme-ul li{
	position:relative;
	float:left;
	margin-right:0.24rem;
	width:5.58rem;
	height:3.48rem;
	border:0.01rem solid #2a5b7d;
	border-radius:0.04rem;
	overflow:hidden;
	cursor:pointer;
}

.theme-ul li img{
	width:100%;
	height:100%;
}

.theme-ul li:hover:after,.theme-ul li.active:after{
	content:'';
	width:100%;
	height:100%;
	left:0;
	top:0;
	position:absolute;
    box-shadow: 0 0 0.35rem #6dbdde inset;
}

.theme-op{
	padding:0.35rem 0;
	text-align:center;
}

.theme-op .sham-btn{
	width:1.28rem;
}

.theme-op .sham-btn:first-child{
	margin-right:1.7rem;
}

/**新增删除弹窗**/
.add-delete{
	width:6rem;
	padding-top:0.12rem;
}

.add-delete-input{
	margin: 0.2rem 0;
    text-align: center;
}

.add-delete-input input{
